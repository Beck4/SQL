-To organize brands alphabetically 
SELECT * FROM groceries ORDER BY cost asc;

-To get the sum of all the inventory 
SELECT sum(inventory) FROM groceries;

-To see what you have the most of in yout iventory
SELECT max(inventory) from groceries;
